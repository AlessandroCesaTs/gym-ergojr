<?xml version="1.0" ?>
<robot name="puck" xmlns:xacro="http://www.ros.org/wiki/xacro">
    <xacro:include filename="ergojr-macros.xacro.xml"/>
    <xacro:property name="mass" value=".1"/>

    <link name="2_box">
        <xacro:box2 sizex=".03" sizey=".03" sizez=".03" color="blue"/>
        <inertial>
            <mass value="${mass}"/>
            <inertia ixx="1" iyy="1" izz="1"/>
        </inertial>
    </link>


</robot>
